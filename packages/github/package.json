{
  "name": "@graphql-inspector/github",
  "version": "3.4.11",
  "type": "module",
  "description": "GraphQL Inspector's Probot. Compare GraphQL Schemas, check documents, find breaking changes, find similar types.",
  "repository": {
    "type": "git",
    "url": "kamilkisiela/graphql-inspector",
    "directory": "packages/github"
  },
  "author": {
    "name": "Kamil Kisiela",
    "email": "kamil.kisiela@gmail.com",
    "url": "https://github.com/kamilkisiela"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./*": {
      "require": {
        "types": "./dist/typings/*.d.cts",
        "default": "./dist/cjs/*.js"
      },
      "import": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "default": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "keywords": [
    "graphql",
    "graphql-inspector",
    "tools",
    "probot",
    "probot-plugin",
    "github"
  ],
  "scripts": {
    "build": "bob build",
    "clean": "rm -rf dist/",
    "deploy": "now --docker",
    "dev": "pnpm build && pnpm start",
    "prebuild": "pnpm clean",
    "prepack": "bob prepack",
    "prepublishOnly": "pnpm build",
    "start": "probot run ./dist/index.js",
    "test": "vitest ."
  },
  "peerDependencies": {
    "graphql": "^14.0.0 || ^15.0.0 || ^16.0.0",
    "probot": "^9.0.1 || ^10.0.0 || ^11.0.0"
  },
  "dependencies": {
    "@graphql-inspector/core": "workspace:*",
    "@whatwg-node/fetch": "0.8.5",
    "dataloader": "2.2.2",
    "js-yaml": "4.1.0",
    "tslib": "2.5.0"
  },
  "devDependencies": {
    "@types/express": "4.17.17",
    "@types/js-yaml": "4.0.5",
    "@types/node": "18.15.5",
    "probot": "11.4.1",
    "smee-client": "1.2.3"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
