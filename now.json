{
  "name": "graphql-inspector",
  "version": 2,
  "alias": ["graphql-inspector"],
  "env": {
    "LOG_FORMAT": "json",
    "LOG_LEVEL": "debug",
    "APP_ID": "@graphql-inspector-app-id",
    "PRIVATE_KEY": "@graphql-inspector-private-key",
    "WEBHOOK_SECRET": "@graphql-inspector-webhook-secret",
    "FAUNADB_STATS_SECRET": "@graphql-inspector-faunadb-stats"
  },
  "public": false,
  "redirects": [
    {
      "source": "/install",
      "destination": "https://github.com/marketplace/graphql-inspector",
      "statusCode": 301
    },
    {
      "source": "/action",
      "destination": "https://github.com/marketplace/actions/graphql-inspector",
      "statusCode": 301
    },
    {
      "source": "/docs",
      "destination": "/docs/index",
      "statusCode": 301
    }
  ],
  "functions": {
    "api/webhook.js": {
      "maxDuration": 20
    }
  },
  "github": {
    "enabled": false
  }
}
